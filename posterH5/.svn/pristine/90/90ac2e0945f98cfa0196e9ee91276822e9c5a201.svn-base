#landscape{
    display: none;
	position: absolute;
	top:0;
	left:0;
	width:100%;
	height:100%;
	background: url('../img/landscape.jpg') 50% 50%;
	background-size: cover;
	z-index: 99999;
}
.music{
	display: none;
	position: absolute;
	width:0.42rem;
	height: 0.38rem;
	top: 0.2rem;
	right: 0.2rem;	
	z-index: 10;
}
.music_run{
	animation: shake 1.3s infinite;
    -webkit-animation: shake 1.3s infinite;
}
@-webkit-keyframes shake {
    0%,20%{
        -webkit-transform: scale(1);
    }
    
    40%{
    	-webkit-transform: scale(1.2);
    }
    
    100% {
        -webkit-transform: scale(1);
    }
}
.music img{
	position: absolute;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
}
